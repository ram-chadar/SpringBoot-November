package com.example.demo;

public class TechnoSys {
	private String name;
	
	

	public TechnoSys(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
	public void show() {
		System.out.println("hi technosys");
	}
	
}
